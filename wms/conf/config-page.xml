<?xml version="1.0" encoding="utf-8"?>
<plume-page>
    <!--配置HTML容器的初始化方法-->
    <index-init>pageInit</index-init>

    <!--配置模板-->
    <page>
        <!--配置模板别名-->
        <page-name>index</page-name>
        <!--配置模板路径,从根目录开始写路径,非相对路径-->
        <page-url>/wms/index.html</page-url>
        <!--配置模板初始化函数,与js中的方法名对应,无参数,所有的初始化plume模板函数都在框架js中实现-->
        <init>indexInit</init>
    </page>

    <page>
        <page-name>popTips</page-name>
        <page-url>/wms/pages/popTips.html</page-url>
        <init>popTips_init</init>
    </page>

</plume-page>